package Exam_02;

public class HumanSwitch {

	public static void main(String[] args) {
		//고객의 등급에 따라 고객 등급을 표현함.
		char grade = 'A';
		
		switch (grade) {
		
		case 'A' :
			System.out.println("당신은 A등급입니다.");
		case 'B' :
			System.out.println("당신은 B등급입니다.");
		case 'C' :
			System.out.println("당신은 C등급입니다.");
		default :
			System.out.println("당신은 회원이 아닙니다.");
		}
	}

}
