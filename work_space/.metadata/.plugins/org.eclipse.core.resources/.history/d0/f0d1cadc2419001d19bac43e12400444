package Exam_04;

public class Exercise07 {

	public static void main(String[] args) {
		int max = 0;
		int [] array = {1,5,3,8,2};
		max = array[0];
		
		for (int i = 1 ; i < array.length ; i++) {
			if (max < array[i]) {
				max = array[i];
			}
		}
		
		System.out.printf("array[]의 가장 큰 수는 : %d", max);

	}

}
