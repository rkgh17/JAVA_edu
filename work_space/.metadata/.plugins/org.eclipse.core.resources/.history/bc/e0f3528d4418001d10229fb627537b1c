package Exam_02;

public class practice4 {

	public static void main(String[] args) {
		int i = 1;
		int dice1 = ((int) (Math.random() * 10) + 1) % 6;
		int dice2 = ((int) (Math.random() * 10) + 1) % 6;

		while ((dice1 + dice2) == 5) {
			System.out.printf("주사위가 %d, %d가 나왔습니다! \n", dice1, dice2);
		}

	}

}
